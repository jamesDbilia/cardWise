generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model Ebay{
    id                String  @id @default(cuid())
}
model Psa{
    id                String  @id @default(cuid())
}
model Bsg{
    id                String  @id @default(cuid())
}


